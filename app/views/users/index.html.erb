<h1>Index</h1>
<%= link_to "Create new user", new_user_path %>
<table>
<head>
  <tr>
    <th>Name</th>
    <th>Email</th>
	</tr>
    </head>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= link_to "destroy", destroy_user_path(user.id), method: :delete %></td>
        <td><%= link_to "edit", edit_user_path(user.id), method: :get %> </td>

      </tr>
    <% end %>
  </tbody>
</table>