<center>
  <%= form_for @collage do |form| %>
    Create new collage<br>
    <%= form.text_field :name %><br>
    <br>
    <%= form.text_field :enrollno %><br>
    <%= form.text_field :standard %><br>
    <%= form.text_field :subject %><br>
    <%= form.select :standard, ["1", "2", "3"] %>
    <%= form.check_box :enrollno %>
    <%= form.label :enrollno, "I have a enrollno" %>
    <%= form.submit %>
  <% end %>
</center>
